{
  "name": "genkidama",
  "version": "1.0.5",
  "description": "Crypto library for encryption/decryption of files/streams in node",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run lint && npm run build && npm run test",
    "build": "tsc",
    "lint": "tslint 'lib/**/*.ts' && tslint 'test/**/*.ts' && tslint 'example/**/*.ts'",
    "test": "ts-node node_modules/.bin/jasmine test/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/7jpsan/genkidama.git"
  },
  "keywords": [
    "crypto",
    "typescript",
    "aes",
    "cbc",
    "ctr",
    "node"
  ],
  "author": "Paulo Soares",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/7jpsan/genkidama/issues"
  },
  "homepage": "https://github.com/7jpsan/genkidama#readme",
  "dependencies": {
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.0",
    "@types/lodash": "^4.14.163",
    "@types/node": "^14.14.6",
    "jasmine": "^3.6.2",
    "keypair": "^1.0.1",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  }
}
